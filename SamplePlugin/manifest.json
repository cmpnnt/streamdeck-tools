{
  "$schema": "https://schemas.elgato.com/streamdeck/plugins/manifest.json",
  "Actions": [
    {
      "Icon": "Images/icon",
      "Name": "SDTools Test",
      "Controllers": ["Keypad", "Encoder"],
      "Encoder": {
        "layout": "$B1",
        "TriggerDescription": {
          "Push": "Play / Pause",
          "Rotate": "Adjust Volume",
          "Touch": "Play / Pause",
          "LongTouch": "Skip Track"
        }
      },
      "States": [
        {
          "Image": "Images/pluginAction",
          "TitleAlignment": "middle",
          "FontSize": 12
        }
      ],
      "SupportedInMultiActions": true,
      "Tooltip": "Sample Plugin to Test SDTools",
      "PropertyInspectorPath": "PropertyInspector/Sample.html",
      "UUID": "com.test.sdtools.sampleplugin"
    }
  ],
  "Author": "BarRaider",
  "Description": "Sample Plugin to test SDTools",
  "Name": "SDTools Sample Plugin",
  "Icon": "Images/pluginIcon",
  "URL": "https://barraider.com/",
  "Version": "0.1.0.1",
  "CodePath": "com.test.sdtools.exe",
  "Category": "SDTools Sample Plugin",
  "CategoryIcon": "Images/categoryIcon",
  "OS": [
    {
      "Platform": "windows",
      "MinimumVersion": "10"
    }
  ],
  "SDKVersion": 2,
  "Software": {
    "MinimumVersion": "6.4"
  },
  "ApplicationsToMonitor": {
    "windows": [
      "notepad.exe",
      "calc.exe"
    ]
  },
  "UUID": "com.test.sdtools"
}